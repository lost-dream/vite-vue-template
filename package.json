{
  "name": "gods-pen-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "lint:stylelint": "stylelint \"**/*.{vue,postcss,css,scss}\" --fix",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx,js,jsx,css,scss,postcss}\" --fix",
    "lint:lint-staged": "lint-staged -c ./lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "lint": "pnpm run lint:eslint && pnpm run lint:stylelint",
    "preview": "vite preview",
    "prepare": "husky install",
    "commit": "czg"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.3",
    "@types/node": "^20.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@unocss/transformer-directives": "^0.51.13",
    "@vitejs/plugin-vue": "^4.1.0",
    "czg": "^1.6.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.20.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.13.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "sass": "^1.62.1",
    "stylelint": "^15.6.2",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.0.2",
    "unocss": "^0.51.13",
    "vite": "^4.3.2",
    "vue-tsc": "^1.4.2"
  }
}
